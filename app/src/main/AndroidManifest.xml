            android:name=".TodoActitivites.WorkActivity"
            android:exported="false" />
        <activity
            android:name=".TodoActitivites.StudyActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.ResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />

    </application>

</manifest>