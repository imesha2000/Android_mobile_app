        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyReadyMe"
        tools:targetApi="31">
        <activity
            android:name=".Actitivties.PrivacyActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.AboutActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Actitivties.Map"
            android:exported="false" />
        <activity
            android:name=".TodoActitivites.TravelActivity"
            android:exported="false" />
        <activity
            android:name=".TodoActitivites.PersonalActivity"
